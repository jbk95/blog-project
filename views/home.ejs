<%-include("partials/header"); %>
<h1>Home</h1>
<p><%= startingContent %></p>
<% posts.forEach(function(element){ %>
  <h1><%= element.title%></h1>
  <% if (element.content.length > 100) { %>
    <p>
      <%= element.content.slice(0,100)+ "..." %>
      <a href="<%= "/posts/" + element.title %>">Read more.</a>
    </p>

  <%} else { %>
    <p><%= element.content%></p>
  <%}%>
<% }); %>
<%-include("partials/footer"); %>
